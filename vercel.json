{
    "builds": [
      {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "staticfiles"
        }
      },
      {
        "src": "JaWa/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "runtime": "python3.9"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "JaWa/wsgi.py"
      }
    ],
    "outputDirectory": "staticfiles"
  }
  